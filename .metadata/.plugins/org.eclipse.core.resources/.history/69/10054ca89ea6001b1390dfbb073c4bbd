package circuits;

public class TrueGate extends Gate  {
	private static final String TRUE_GATE = "T";
	private static Gate instance = null;
	
	private TrueGate(Gate[] inGates) {
		super(inGates);
		// TODO Auto-generated constructor stub
	}
	
	public static Gate instance() {
		if(instance == null)
			instance = new Gate(super(inGates));
		return instance;
	}

	@Override
	protected boolean func(boolean[] inValues) {
		// TODO Auto-generated method stub
		return false;
	}

	@Override
	public String getName() {
		return TRUE_GATE;
	}

	@Override
	public Gate simplify() {
		// TODO Auto-generated method stub
		return null;
	}
}
