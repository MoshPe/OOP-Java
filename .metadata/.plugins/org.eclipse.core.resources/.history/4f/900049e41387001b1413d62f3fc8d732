package a;

public class MainForTimer {

	public static void main(String[] args) {
		Timer time = new Timer(10,0);
		System.out.println("The time is " + time.toString());
		System.out.println("The total seconds are: " + time.totalSeconds());
		time.tickDown();
		System.out.println("The time is " + time.toString());
		
	}
}
