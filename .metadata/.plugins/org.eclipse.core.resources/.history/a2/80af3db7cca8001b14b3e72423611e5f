package images;

public class Transpose extends ImageDecorator{
	private Image base;
	public Transpose(Image base) {
		super(base.getWidth(),base.getHeight());
		this.base = base;	
	}
	
	@Override
	public RGB get(int x, int y) {
		return base.get(y, x);
	}
	
	public static void main(String[] argc) {
		Image i = new Invert(new Circle(120, 60, RGB.RED, RGB.BLACK));
		//Image i2 = new Circle(120, 60, RGB.RED, RGB.BLACK);
		Displayer.display(i);
	}

}
