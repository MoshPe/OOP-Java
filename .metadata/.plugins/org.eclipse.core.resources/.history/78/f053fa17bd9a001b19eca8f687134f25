package Game;

import java.util.Scanner;

public class Game extends Board {
	protected Player[] players;
	protected Scanner s;
	
	public Game(int n,int m, Player p1, Player p2) {
		super(n,m);
		this.players = new Player[2];
		players[0] = p1;
		players[1] = p2;
	}

protected boolean doesWin(int i, int j) {	return (i==0 && j==0); }






}
