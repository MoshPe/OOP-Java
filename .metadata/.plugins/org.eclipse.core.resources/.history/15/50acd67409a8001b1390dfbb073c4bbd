package fghfgh;

public class MostCommonReporter implements Reporter{
	private int arr[];
	
	public MostCommonReporter(int num) {
		arr = new int[num];
	}
	
	@Override
	public void addNumber(int num) {
		arr[num]++;
		
	}

	@Override
	public int report() {
		int max = 0,index;
		for (int i = 0; i < arr.length; i++) {
			if(arr[i] > max)
				max = arr[i];
				index = il
			
		}
	}

}
