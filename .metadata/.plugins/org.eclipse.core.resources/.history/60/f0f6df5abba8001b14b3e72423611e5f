package images;

public class RGB {
	private double blue, green, red;

	public RGB(double green, double red, double blue) {
		this.red = red;
		this.green = green;
		this.blue = blue;
	}

	public RGB(double grey) {

	}

	public double getRed() { return red; }

	public double getBlue() { return blue; }

	public double getGreen() { return green; }

}
